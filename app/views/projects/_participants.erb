<h3>Participants</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th>User Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>

    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>